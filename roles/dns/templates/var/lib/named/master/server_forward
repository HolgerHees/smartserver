$TTL 2d
@		IN SOA		{{server_name}}.	root.{{server_name}}. (
				2018041800	; serial
				3h		; refresh
				1h		; retry
				1w		; expiry
				1d )		; minimum

{{server_domain}}.	IN NS		ns.{{server_domain}}.
{{server_domain}}.	IN A		{{server_ip}}
ns		IN A		{{server_ip}}
local		IN A		{{server_ip}}
cloud		IN A		{{server_ip}}
office		IN A		{{server_ip}}
www		IN A		{{server_ip}}
wetter		IN A		{{server_ip}}
