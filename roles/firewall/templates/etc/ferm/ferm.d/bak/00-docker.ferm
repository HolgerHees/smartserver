domain (ip ip6) {
    table filter {
        chain ("/^NETAVARK.*/" DOCKER DOCKER-USER DOCKER-INGRESS DOCKER-ISOLATION-STAGE-1 DOCKER-ISOLATION-STAGE-2 FORWARD) @preserve;
    }

    table nat {
        chain ("/^NETAVARK.*/" DOCKER DOCKER-USER DOCKER-INGRESS PREROUTING OUTPUT POSTROUTING) @preserve;
    }
} 
