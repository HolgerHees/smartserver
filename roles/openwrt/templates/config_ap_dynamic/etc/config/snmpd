config agent
        option agentaddress UDP:161,UDP6:161

config agentx
        option agentxsocket /var/run/agentx.sock

config com2sec public
        option secname ro
        option source default
        option community public

config com2sec private
        option secname rw
        option source localhost
        option community private

config com2sec6 public6
        option secname ro
        option source default
        option community public

config com2sec6 private6
        option secname rw
        option source localhost
        option community private

config group public_v1
        option group public
        option version v1
        option secname ro

config group public_v2c
        option group public
        option version v2c
        option secname ro

config group public_usm
        option group public
        option version usm
        option secname ro

config group private_v1
        option group private
        option version v1
        option secname rw

config group private_v2c
        option group private
        option version v2c
        option secname rw

config group private_usm
        option group private
        option version usm
        option secname rw

config view all
        option viewname all
        option type included
        option oid .1

config access public_access
        option group public
        option context none
        option version any
        option level noauth
        option prefix exact
        option read all
        option write none
        option notify none

config access private_access
        option group private
        option context none
        option version any
        option level noauth
        option prefix exact
        option read all
        option write all
        option notify all

