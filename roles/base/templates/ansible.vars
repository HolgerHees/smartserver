{
  {% for name, config in default_variables.items() %}
    {% if name in vars %}
        'default_vars_{{name}}': 'custom',
    {% else %}
        {% if ('optional' not in config or not config['optional']) %}
            {% if 'default' in config %}
                '{{name}}': {{config['default'] | to_json}},
            {% else %}
                'default_vars_{{name}}': 'missing',
            {% endif %}
        {% endif %}
    {% endif %}
  {% endfor %}
}

